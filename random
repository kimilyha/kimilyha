import asyncio
import random

async def random_command(command):
    try:
        # Разбиваем команду на части
        parts = command.split()
        
        # Проверяем, что команда правильного формата и содержит два числа
        if len(parts) != 3 or parts[0] != '.random':
            return "Неправильный формат команды. Используйте: .random n1 n2"
        
        n1 = int(parts[1])
        n2 = int(parts[2])
        
        # Проверяем, что первое число меньше второго
        if n1 >= n2:
            return "Первое число должно быть меньше второго."
        
        # Генерируем случайное число в заданном диапазоне
        random_number = random.randint(n1, n2)
        
        return f"Случайное число между {n1} и {n2}: {random_number}"
    
    except ValueError:
        return "Пожалуйста, укажите два целых числа после команды .random"

async def main():
    while True:
        command = input("Введите команду (.random n1 n2): ")
        if command == "exit":
            break
        result = await random_command(command)
        print(result)

# Запускаем асинхронную функцию
asyncio.run(main())
